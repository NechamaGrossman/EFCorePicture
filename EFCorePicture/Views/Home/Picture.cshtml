@using EFCorePicture.Data;
@model Picture
<div class="row">
    <div class="col-md-6 col-md-offset-3 well">
        <div class="well">
            <h1>@Model.Title</h1>
            <img src="@Model.PictureURL" />
            <h5>Listed on @Model.Date.ToLongDateString()</h5>
            <button id="button" value="@Model.Id"><img width="40" height="40" src="data:image/png;base64," /></button>
            <h5 id="likes">Likes @Model.Likes</h5>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/js/Picture.js"></script>
}